# Ignore bundler config.
/.bundle

# Ignore all environment files.
/.env*
!/.env*.erb

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore pidfiles, but keep the directory.
/tmp/pids/*
!/tmp/pids/
!/tmp/pids/.keep

# Ignore storage (uploaded files and SQLite databases in development/test).
/storage/*
!/storage/.keep

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore node_modules
/node_modules

# Ignore yarn files
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore assets builds (compiled CSS/JS)
/app/assets/builds/*
!/app/assets/builds/.keep

# Ignore bootsnap cache
/tmp/cache/bootsnap

# Ignore byebug history
.byebug_history

# Ignore IDE files
.idea/
.vscode/
*.swp
*.swo
*~

# Ignore OS files
.DS_Store
Thumbs.db
